{
    "name": "davidjegat/jimport-bundle",
    "type": "symfony-bundle",
    "keywords": ["assetic", "javascript", "import", "parser"],
    "homepage": "https://github.com/davidjegat/JImportBundle",
    "description": "A simple assetic files parser for import javascript files easily.",
    "require": {
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "php": ">=5.3.2",
        "kriswallsmith/assetic": ">=1.1.0-alpha4,<1.2-dev",
        "symfony/assetic-bundle": "2.1.*",
        "twig/twig": "1.12.*",
        "twig/extensions": "1.0.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "David Jegat",
            "email": "david.jegat@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "DavidJegat\\JImportBundle": "" }
    },
    "target-dir": "DavidJegat/JImportBundle"
}
